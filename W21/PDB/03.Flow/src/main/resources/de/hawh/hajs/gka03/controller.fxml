<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.hawh.hajs.gka03.controllers.MainController">
<children>
    <Button id="loadGraph" fx:id="loadGraph" alignment="CENTER" contentDisplay="CENTER" onAction="#handleLoadGraphBtn" text="Load graph" textAlignment="CENTER" />
    <Button id="generateGraph" fx:id="generateGraph" alignment="CENTER" contentDisplay="CENTER" onAction="#handleGenerateGraphBtn" text="Generate Graph" textAlignment="CENTER" />
    <Button id="saveGraph" fx:id="saveGraph" alignment="CENTER" contentDisplay="CENTER" disable="true" onAction="#saveGraph" text="Save Graph" textAlignment="CENTER" />
</children>
<padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
</padding>
</VBox>
